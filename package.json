{
  "name": "@homeferences/website",
  "version": "0.0.0-development",
  "description": "This is a Gatsby based static website, which renders the List of Homeferences.",
  "homepage": "https://github.com/homeferences/website#readme",
  "dependencies": {
    "date-fns": "^2.11.1",
    "framer-motion": "^1.10.2",
    "gatsby": "^2.20.6",
    "gatsby-image": "^2.3.1",
    "gatsby-plugin-emotion": "^4.2.1",
    "gatsby-plugin-manifest": "^2.3.2",
    "gatsby-plugin-offline": "^3.1.1",
    "gatsby-plugin-react-helmet": "^3.2.1",
    "gatsby-plugin-sharp": "^2.5.3",
    "gatsby-plugin-theme-ui": "^0.3.0",
    "gatsby-transformer-sharp": "^2.4.2",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^5.2.1",
    "theme-ui": "^0.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-angular": "^8.3.4",
    "eslint": "^6.8.0",
    "eslint-config-gatsby-standard": "^2.2.0",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.8",
    "prettier": "^2.0.1"
  },
  "keywords": [
    "conferences"
  ],
  "author": "Homeferences Contributors",
  "license": "CC0-1.0",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/homeferences/website"
  },
  "bugs": {
    "url": "https://github.com/homeferences/website/issues"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,md,json,yaml,yml}": [
      "prettier --write"
    ]
  }
}
