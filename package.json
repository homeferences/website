{
  "name": "@homeferences/website",
  "version": "0.0.0-development",
  "description": "This is a Gatsby based static website which renders the List of Homeferences.",
  "homepage": "https://github.com/homeferences/website#readme",
  "dependencies": {
    "date-fns": "^2.15.0",
    "framer-motion": "^1.11.0",
    "gatsby": "^2.23.20",
    "gatsby-image": "^2.4.13",
    "gatsby-plugin-emotion": "^4.3.10",
    "gatsby-plugin-manifest": "^2.4.18",
    "gatsby-plugin-offline": "^3.2.9",
    "gatsby-plugin-react-helmet": "^3.3.10",
    "gatsby-plugin-sharp": "^2.6.19",
    "gatsby-plugin-theme-ui": "^0.3.0",
    "gatsby-transformer-sharp": "^2.5.11",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "snyk": "^1.398.1",
    "theme-ui": "^0.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-angular": "^8.3.4",
    "eslint": "^7.2.0",
    "eslint-config-gatsby-standard": "^2.2.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.9",
    "prettier": "^2.0.5"
  },
  "keywords": [
    "conferences"
  ],
  "author": "Homeferences Contributors",
  "license": "CC0-1.0",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/homeferences/website"
  },
  "bugs": {
    "url": "https://github.com/homeferences/website/issues"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,md,json,yaml,yml}": [
      "prettier --write"
    ]
  },
  "snyk": true
}
